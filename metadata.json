{
  "name": "bloombergindustry-windows_smb",
  "version": "0.4.4",
  "author": "Bloomberg Industry Group",
  "summary": "Module that will manage windows SMB shares, server and client SMB configuration",
  "license": "Apache-2.0",
  "source": "https://github.com/bloombergindustry/windows_smb",
  "project_page": "https://github.com/bloombergindustry/windows_smb",
  "issues_url": "https://github.com/bloombergindustry/windows_smb/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.6.0 < 7.0.0"
    },
    {
      "name": "puppetlabs/powershell",
      "version_requirement": ">= 1.0.1 < 3.0.0"
    },
    {
      "name": "puppet/windowsfeature",
